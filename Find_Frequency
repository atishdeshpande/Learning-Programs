#include<iostream>
using namespace std;

int main()
{
    int a[100];
    int ELE, i, n,count=0;
    cout<<"enter the number of element in array "<< endl;
    cin>>n;
    //int a[n];

    cout<<"enter the array elements"<< endl;
    for(int i=0; i<n; i++){
        cout<<"the value of number"<< i+1 << "="<< endl;
        cin>>a[i];
    }

    cout<<"enter the element to find the frequency"<<endl;
    cin>> ELE;
    for(i=0;i<n;i++)
    
    if(a[i]==ELE) 
    count++;
    
    cout<<ELE<<"occurs"<< count <<"times" << endl;
    
    return 0;
}
